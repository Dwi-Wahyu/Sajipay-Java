# Gradle build output
build/
.gradle/
gradlew
gradlew.bat
gradle-wrapper.jar
gradle-wrapper.properties

# Cache dan file sementara Gradle
.gradle/
.gradle/**

# File konfigurasi IDE
.idea/              # Jika menggunakan IntelliJ IDEA
*.iml               # File modul IntelliJ
.vscode/            # Jika menggunakan Visual Studio Code
*.code-workspace    # File workspace VS Code
.classpath          # Jika menggunakan Eclipse
.project            # File proyek Eclipse
.settings/          # Pengaturan proyek Eclipse

# File sementara Java
*.class             # File hasil kompilasi Java
*.jar               # File JAR yang dihasilkan
*.war               # File WAR yang dihasilkan
*.ear               # File EAR yang dihasilkan

# File log
*.log

# File lokal lainnya
bin/                # Folder output build (tergantung IDE)
out/                # Folder output build (tergantung IDE)
target/             # Folder output build (tergantung Maven/Gradle)
dist/               # Folder distribusi (jika ada)

# File spesifik JavaFX
lib/                # Jika Anda menyimpan JavaFX SDK secara lokal
javafx-sdk*/        # Folder JavaFX SDK yang diunduh manual

# File sistem operasi
.DS_Store           # File macOS
Thumbs.db           # File Windows
*.swp               # File sementara Vim
*.bak               # File backup

# File konfigurasi lokal
local.properties    # File konfigurasi lokal Gradle